// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel  Quemada;

nu              nu [ 0 2 -1 0 0 0 0 ] 3.302E-06;

switchOn 0.4;

QuemadaCoeffs
{
    tau0            tau0 [ 1 -1 -2 0 0 0 0 ] 7.160E-03;
    muInf           muInf [ 1 -1 -1 0 0 0 0 ] 4.204E-03;
    lambda          lambda [ 0 0 -1 0 0 0 0 ] 4.367E-03;
    rho 	        rho [1 -3 0 0 0 0 0] 1060;
}

